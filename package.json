{
  "name": "color-js",
  "version": "0.5.0",
  "description": "The convenient way to handle the color in JS",
  "keywords": [
    "color",
    "rgb",
    "rgba",
    "hsl",
    "hsla",
    "red",
    "green",
    "blue",
    "hue",
    "saturation",
    "brightness",
    "lightness",
    "opacity",
    "transparency"
  ],
  "author": "Roman Melnyk <email.rom.melnyk@gmail.com>",
  "bugs": {
    "url": "https://github.com/rom-melnyk/color.js/issues",
    "eamil": "email.rom.melnyk@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/rom-melnyk/color.js"
  },
  "devDependencies": {
    "browserify": "^11.0.1",
    "uglify-js": "^2.4.24",
    "watchify": "^3.3.1"
  },
  "main": "src/color.js",
  "scripts": {
    "dev": "watchify src/color.js -o color.dev.js -d -v",
    "prod": "browserify src/color.js -o color.dev.js && uglifyjs color.dev.js --screw-ie8 -c -m -o color.min.js"
  }
}
